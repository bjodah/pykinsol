   },
   "outputs": [],
   "source": [
    "def f(x, fout):\n",
    "    fout[0] = x[1] - 1 + 1/x[0]\n",
    "    fout[1] = x[1] + 1/(x[0] + 1)\n",
    "\n",
    "def j(x, Jout, fx):\n",
    "    Jout[0, 0] = -1/(x[0]**2)\n",
    "    Jout[0, 1] = 1\n",
    "    Jout[1, 0] = -1/(x[0] + 1)**2\n",
    "    Jout[1, 1] = 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "solve(f, j, [.5, -.5])"
   ]
  }
